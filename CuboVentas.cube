  <MdxScripts>
    <MdxScript dwd:design-time-name="79b63921-1c94-42ef-957e-db456dd9fcef">
      <ID>MdxScript</ID>
      <Name>MdxScript</Name>
      <CreatedTimestamp>0001-01-01T05:00:00Z</CreatedTimestamp>
      <LastSchemaUpdate>0001-01-01T05:00:00Z</LastSchemaUpdate>
      <Commands>
        <Command>
          <Text>/*
The CALCULATE command controls the aggregation of leaf cells in the cube.
If the CALCULATE command is deleted or modified, the data within the cube is affected.
You should edit this command only if you manually specify how the cube is aggregated.
*/
CALCULATE;  
CREATE MEMBER CURRENTCUBE.[DIM CLIENTE].[Edad].[Segmento Edad]
 AS CASE 
    WHEN Edad &lt;= 25 THEN 'Jóvenes (≤25)'
    WHEN Edad &lt;= 35 THEN 'Adultos Jóvenes (26-35)'
    WHEN Edad &lt;= 45 THEN 'Adultos (36-45)'
    WHEN Edad &lt;= 55 THEN 'Adultos Maduros (46-55)'
    WHEN Edad &gt; 55 THEN 'Seniors (&gt;55)'
    ELSE 'No Definido'
END, 
FORMAT_STRING = "dddd, MMMM d, yyyy", 
VISIBLE = 1 ,  DISPLAY_FOLDER = 'Segmento Edad'  ;                
CREATE MEMBER CURRENTCUBE.[Measures].[Meta Volumen Base]
 AS CASE
    -- Meta por nivel de tiempo
    WHEN [Dim Tiempo].[Calendario].CurrentMember.Level.Name = "All" THEN 60000
    WHEN [Dim Tiempo].[Calendario].CurrentMember.Level.Name = "Anio" THEN 15000
    WHEN [Dim Tiempo].[Calendario].CurrentMember.Level.Name = "Mes" THEN 1250
    WHEN [Dim Tiempo].[Calendario].CurrentMember.Level.Name = "Dia" THEN 42
    
    -- Ajustes por zona de cliente (opcional)
    WHEN [Dim Cliente].[Zona].CurrentMember.Name = "Norte" THEN 1500
    WHEN [Dim Cliente].[Zona].CurrentMember.Name = "Sur" THEN 1200
    WHEN [Dim Cliente].[Zona].CurrentMember.Name = "Centro" THEN 1000
    WHEN [Dim Cliente].[Zona].CurrentMember.Name = "Este" THEN 800
    WHEN [Dim Cliente].[Zona].CurrentMember.Name = "Oeste" THEN 900
    
    -- Meta por grupo de producto (opcional)
    WHEN [Dim Producto].[GrupoProd].CurrentMember.Name = "Autobuses" THEN 2000
    WHEN [Dim Producto].[GrupoProd].CurrentMember.Name = "Repuestos" THEN 800
    WHEN [Dim Producto].[GrupoProd].CurrentMember.Name = "Accesorios" THEN 500
    
    -- Meta default
    ELSE 1250
END, 
FORMAT_STRING = "#,##0.00;-#,##0.00", 
BACK_COLOR = CASE
    WHEN [Measures].[Meta Volumen Base] &gt;= 10000 THEN RGB(230, 230, 250)
    WHEN [Measures].[Meta Volumen Base] &gt;= 5000 THEN RGB(240, 248, 255)
    WHEN [Measures].[Meta Volumen Base] &gt;= 1000 THEN RGB(248, 248, 255)
    WHEN [Measures].[Meta Volumen Base] &gt;= 100 THEN RGB(250, 250, 255)
    ELSE RGB(255, 255, 255)
END, 
FORE_COLOR = CASE
    WHEN [Measures].[Meta Volumen Base] &gt;= 10000 THEN RGB(0, 0, 139)
    WHEN [Measures].[Meta Volumen Base] &gt;= 5000 THEN RGB(25, 25, 112)
    WHEN [Measures].[Meta Volumen Base] &gt;= 1000 THEN RGB(70, 130, 180)
    WHEN [Measures].[Meta Volumen Base] &gt;= 100 THEN RGB(100, 149, 237)
    ELSE RGB(176, 196, 222)
END, 
FONT_NAME = "Segoe UI", 
FONT_SIZE = CASE
    WHEN [Measures].[Meta Volumen Base] &gt;= 10000 THEN 12
    WHEN [Measures].[Meta Volumen Base] &gt;= 1000 THEN 11
    ELSE 10
END, 
VISIBLE = 1 ,  DISPLAY_FOLDER = 'Metas y Objetivos' ,  ASSOCIATED_MEASURE_GROUP = 'HECHO VENTA';                
CREATE MEMBER CURRENTCUBE.[Measures].[Volumen Acumulado Anio]
 AS CASE
    WHEN [Dim Tiempo].[Calendario].CurrentMember.Level.Name = "Anio" 
    THEN [Measures].[Volumen Venta]
    WHEN [Dim Tiempo].[Calendario].CurrentMember.Level.Name = "Mes"
    THEN SUM(
        PeriodsToDate([Dim Tiempo].[Calendario].[Anio], [Dim Tiempo].[Calendario].CurrentMember),
        [Measures].[Volumen Venta]
    )
    WHEN [Dim Tiempo].[Calendario].CurrentMember.Level.Name = "Dia"
    THEN SUM(
        PeriodsToDate([Dim Tiempo].[Calendario].[Anio], [Dim Tiempo].[Calendario].CurrentMember),
        [Measures].[Volumen Venta]
    )
    ELSE [Measures].[Volumen Venta]
END, 
FORMAT_STRING = "#,##0.00;-#,##0.00", 
BACK_COLOR = CASE
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 10000 THEN RGB(240, 255, 240)
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 5000 THEN RGB(245, 255, 245)
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 2500 THEN RGB(255, 248, 220)
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 1000 THEN RGB(255, 228, 225)
    WHEN [Measures].[Volumen Acumulado Anio] &gt; 0 THEN RGB(255, 240, 245)
    ELSE RGB(248, 248, 248)
END, 
FORE_COLOR = CASE
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 10000 THEN RGB(0, 100, 0)
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 5000 THEN RGB(34, 139, 34)
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 2500 THEN RGB(255, 140, 0)
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 1000 THEN RGB(255, 69, 0)
    WHEN [Measures].[Volumen Acumulado Anio] &gt; 0 THEN RGB(220, 20, 60)
    ELSE RGB(128, 128, 128)
END, 
FONT_NAME = "Segoe UI", 
FONT_SIZE = CASE
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 10000 THEN 12
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 5000 THEN 11
    ELSE 10
END, 
FONT_FLAGS = CASE
    WHEN [Measures].[Volumen Acumulado Anio] &gt;= 5000 THEN 1
    ELSE 0
END, 
NON_EMPTY_BEHAVIOR = { [Volumen Venta] }, 
VISIBLE = 1 ,  DISPLAY_FOLDER = 'Análisis de Volumen' ,  ASSOCIATED_MEASURE_GROUP = 'HECHO VENTA';                
CREATE MEMBER CURRENTCUBE.[Measures].[Volumen Promedio Mensua]
 AS CASE
    WHEN [Measures].[Hecho Venta Count] = 0 THEN 0
    WHEN ISEMPTY([Measures].[Hecho Venta Count]) THEN 0
    WHEN [Measures].[Hecho Venta Count] &lt;= 0 THEN 0
    ELSE [Measures].[Volumen Venta] / [Measures].[Hecho Venta Count]
END, 
FORMAT_STRING = "#,##0.00;-#,##0.00", 
BACK_COLOR = CASE
    WHEN [Measures].[Volumen Promedio Mensual] &gt;= 100 THEN RGB(240, 255, 240)
    WHEN [Measures].[Volumen Promedio Mensual] &gt;= 50 THEN RGB(255, 248, 220)
    WHEN [Measures].[Volumen Promedio Mensual] &gt; 0 THEN RGB(255, 240, 240)
    ELSE RGB(248, 248, 248)
END, 
FORE_COLOR = CASE
    WHEN [Measures].[Volumen Promedio Mensual] &gt;= 100 THEN RGB(0, 128, 0)
    WHEN [Measures].[Volumen Promedio Mensual] &gt;= 50 THEN RGB(255, 165, 0)
    WHEN [Measures].[Volumen Promedio Mensual] &gt; 0 THEN RGB(255, 0, 0)
    ELSE RGB(128, 128, 128)
END, 
FONT_NAME = "Segoe UI", 
FONT_SIZE = CASE
    WHEN [Measures].[Volumen Promedio Mensual] &gt;= 100 THEN 11
    ELSE 10
END, 
FONT_FLAGS = CASE
    WHEN [Measures].[Volumen Promedio Mensual] &gt;= 100 THEN 1
    ELSE 0
END, 
NON_EMPTY_BEHAVIOR = { [Volumen Venta] }, 
VISIBLE = 1 ,  DISPLAY_FOLDER = 'Análisis de Volumen' ,  ASSOCIATED_MEASURE_GROUP = 'HECHO VENTA';                 
CREATE MEMBER CURRENTCUBE.[Measures].[Ratio Rentabilidad Porcentaje]
 AS CASE 
    WHEN [Measures].[Total Venta] = 0 THEN 0.00
    WHEN ISEMPTY([Measures].[Total Venta]) THEN 0.00
    ELSE (([Measures].[Total Venta] - [Measures].[Costo Total]) / [Measures].[Total Venta]) * 100
END, 
FORMAT_STRING = "Percent", 
BACK_COLOR = 8404992 /*R=0, G=64, B=128*/ , 
FORE_COLOR = 16777088 /*R=128, G=255, B=255*/ , 
NON_EMPTY_BEHAVIOR = { [Valor Venta] }, 
VISIBLE = 1 ,  DISPLAY_FOLDER = 'Medidas Calculadas' ,  ASSOCIATED_MEASURE_GROUP = 'HECHO VENTA';              
CREATE MEMBER CURRENTCUBE.[Measures].[Costo Total Calculado]
 AS CASE
    WHEN ISEMPTY([Measures].[Costo Total]) THEN [Measures].[Valor Venta] * 0.70
    WHEN [Measures].[Costo Total] &lt;= 0 THEN [Measures].[Valor Venta] * 0.70
    ELSE [Measures].[Costo Total]
END, 
FORMAT_STRING = "Currency", 
BACK_COLOR = CASE
    WHEN [Measures].[Costo Total Real] &gt;= 5000 THEN RGB(255, 240, 245)
    WHEN [Measures].[Costo Total Real] &gt;= 2000 THEN RGB(255, 248, 220)
    WHEN [Measures].[Costo Total Real] &gt;= 500 THEN RGB(255, 255, 240)
    ELSE RGB(255, 255, 255)
END, 
FORE_COLOR = CASE
    WHEN [Measures].[Costo Total Real] &gt;= 5000 THEN RGB(139, 0, 0)
    WHEN [Measures].[Costo Total Real] &gt;= 2000 THEN RGB(255, 69, 0)
    WHEN [Measures].[Costo Total Real] &gt;= 500 THEN RGB(255, 140, 0)
    ELSE RGB(0, 0, 0)
END, 
FONT_NAME = "Segoe UI", 
FONT_SIZE = CASE
    WHEN [Measures].[Costo Total Real] &gt;= 5000 THEN 11
    ELSE 10
END, 
FONT_FLAGS = CASE
    WHEN [Measures].[Costo Total Real] &gt;= 5000 THEN 1
    ELSE 0
END, 
NON_EMPTY_BEHAVIOR = { [Valor Venta] }, 
VISIBLE = 1 ,  DISPLAY_FOLDER = 'Costos' ,  ASSOCIATED_MEASURE_GROUP = 'HECHO VENTA';                 
CREATE MEMBER CURRENTCUBE.[Measures].[Ganancia Bruta]
 AS [Measures].[Total Venta] - [Measures].[Costo Total], 
FORMAT_STRING = "Currency", 
NON_EMPTY_BEHAVIOR = { [Valor Venta] }, 
VISIBLE = 1 ,  DISPLAY_FOLDER = 'Costos y Rentabilidad' ,  ASSOCIATED_MEASURE_GROUP = 'HECHO VENTA'; </Text>
        </Command>
      </Commands>
    </MdxScript>
  </MdxScripts>
  <Kpis>
    <Kpi dwd:design-time-name="684bb163-8078-45bc-93ad-1a16734ebd2a">
      <ID>KPI</ID>
      <Name>KPI_Ratio_Rentabilidad</Name>
      <Description>KPI que mide la rentabilidad de ventas usando la fórmula: ((Ventas - Costo) / Ventas) × 100. Meta: 25%</Description>
      <DisplayFolder>KPIs Rentabilidad</DisplayFolder>
      <AssociatedMeasureGroupID>HECHO VENTA</AssociatedMeasureGroupID>
      <Value>CASE
    WHEN ISEMPTY([Measures].[Ratio Rentabilidad Porcentaje]) THEN NULL
    WHEN [Measures].[Ratio Rentabilidad Porcentaje] THEN [Measures].[Ratio Rentabilidad Porcentaje]
END</Value>
      <Goal>25.00</Goal>
      <Status>CASE
    WHEN ISEMPTY([Measures].[Ratio Rentabilidad Porcentaje]) THEN NULL
    WHEN ISEMPTY([Measures].[KPI_Ratio_Rentabilidad Goal]) THEN NULL
    WHEN [Measures].[Ratio Rentabilidad Porcentaje] &gt;= [Measures].[KPI_Ratio_Rentabilidad Goal] THEN 1
    WHEN [Measures].[Ratio Rentabilidad Porcentaje] &gt;= ([Measures].[KPI_Ratio_Rentabilidad Goal] * 0.60) THEN 0
    ELSE -1
END</Status>
      <Trend>CASE
    WHEN ISEMPTY([Measures].[Ratio Rentabilidad Porcentaje]) THEN NULL
    WHEN ISEMPTY((
        [Measures].[Ratio Rentabilidad Porcentaje], 
        ParallelPeriod([Dim Tiempo].[Calendario].[Anio], 1, [Dim Tiempo].[Calendario].CurrentMember)
    )) THEN 0
    WHEN [Measures].[Ratio Rentabilidad Porcentaje] &gt; 
         ([Measures].[Ratio Rentabilidad Porcentaje], 
          ParallelPeriod([Dim Tiempo].[Calendario].[Anio], 1, [Dim Tiempo].[Calendario].CurrentMember)) * 1.02
    THEN 1
    WHEN [Measures].[Ratio Rentabilidad Porcentaje] &lt; 
         ([Measures].[Ratio Rentabilidad Porcentaje], 
          ParallelPeriod([Dim Tiempo].[Calendario].[Anio], 1, [Dim Tiempo].[Calendario].CurrentMember)) * 0.98
    THEN -1
    ELSE 0
END</Trend>
      <Weight>1</Weight>
      <TrendGraphic>Standard Arrow</TrendGraphic>
      <StatusGraphic>Traffic Light</StatusGraphic>
      <CurrentTimeMember></CurrentTimeMember>
    </Kpi>
  </Kpis>
